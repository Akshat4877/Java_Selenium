package Array;

public class Palindrome {
	
	public static void main(String[] args) {
		
		int r,sum=0;
		int n=1510;
		int temp=n;
	
		while(n>0) {
			
		    r=n%10;
			sum=(sum*10)+r;
			n=n/10;	
		}
		
		if(temp==sum) {
			System.out.println("It is Palindrome");
		}
		else {
			System.out.println("No It is not Palindrome");
		}		
		
	}

}
