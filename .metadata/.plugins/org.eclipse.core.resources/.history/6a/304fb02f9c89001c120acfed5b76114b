package TestNg;

public class CrossBrowser {
	
	@org.testng.annotations.Test
	public void Test1() {
		
		System.out.println("Hi I am in Test 1 |"+Thread.currentThread().getId());
		
	}
	
	
	@org.testng.annotations.Test
public void Test2() {
		System.out.println("Hi I am in Test 2 |"+Thread.currentThread().getId());

		
	}

}
